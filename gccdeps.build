# should start in gccdeps dir

START=$PWD

cd $START/gmp && echo 'gmp' && rm -rf *
$BUILDS/gccdeps/gmp.build

cd $START/mpfr && echo 'mpfr' && rm -rf *
$BUILDS/gccdeps/mpfr.build

cd $START/mpc && echo 'mpc' && rm -rf *
$BUILDS/gccdeps/mpc.build

cd $START/isl && echo 'isl' && rm -rf *
$BUILDS/gccdeps/isl.build

cd $START/cloog && echo 'cloog' && rm -rf *
$BUILDS/gccdeps/cloog.build

cd $START
