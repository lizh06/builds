( CFLAGS="-g -DFEAT_CONCEAL" ./configure --prefix=$U2 \
  --with-compiledby=lizh --with-features=normal \
  --enable-fail-if-missing \
  --enable-pythoninterp=dynamic \
  --enable-python3interp=dynamic \
  --enable-rubyinterp=dynamic \
  --enable-cscope --enable-sniff \
  --enable-multibyte --disable-nls \
  --disable-acl --disable-gui --disable-gpm \
  --disable-selinux --disable-smack \
  --without-x && make -j $CC_LEVEL ) 2>&1 | tee build.log
