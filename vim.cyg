( STRIP=/bin/true CFLAGS='-g -DFEAT_CONCEAL' \
  ./configure --prefix=$u2 \
  --with-compiledby=lizh --with-features=normal \
  --enable-fail-if-missing \
  --enable-pythoninterp=dynamic \
  --enable-multibyte \
  --disable-acl --disable-nls \
  --disable-gui --disable-sysmouse \
  --without-gnome --without-x &&
  make -j $CC_LEVEL && make install && make distclean
) 2>&1 | tee build.log
